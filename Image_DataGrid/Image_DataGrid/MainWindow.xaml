<Window x:Class="Image_DataGrid.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Image_DataGrid"
        mc:Ignorable="d"
        Title="ImageApp" Height="350" Width="525" Background="Wheat" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="300*"/>
            <RowDefinition Height="18*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="1">
            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Left">
                <TextBox Width="250" Text="Contact : https://github.com/MMovasaghi" Foreground="Black" Background="{x:Null}"/>
            </StackPanel>
        </Grid>

    </Grid>
</Window>
